<?xml version="1.0" encoding="UTF-8"?>

<beans:beans xmlns="http://www.springframework.org/schema/security"
    xmlns:beans="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                        http://www.springframework.org/schema/security http://www.springframework.org/schema/security/spring-security-3.1.xsd">

	<!-- Authentication service -->
	<beans:bean id="userService" class="it.micronixnetwork.tcd.service.TCDUserService">
   	<beans:property name="sessionFactory" ref="sessionFactory"/>
   	<beans:property name="md5coding" value="true"/>
  	</beans:bean>
  	
  	
	<!-- Configurazioni Extension -->
	<beans:import resource="classpath*:it/micronixnetwork/**/spring_extension.xml" />
	
</beans:beans>
